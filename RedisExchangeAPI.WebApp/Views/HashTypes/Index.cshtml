@model Dictionary<string, string>

@{
    ViewData["Title"] = "Index";
}

<h1>hash types</h1>

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="key" />
    <input type="text" name="val" />
    <button type="submit">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h4>@item.Key -> @item.Value <a asp-action="DeleteItem" asp-route-name="@item">Sil</a></h4>
}